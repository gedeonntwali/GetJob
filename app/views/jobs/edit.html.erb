<h1> Edit Job Post </h1>

<%= form_tag "/jobs/#{@job.id}", method: :patch do%>

  <form> 

    <div>
      Title: <input type="text" name="title" value="<%=@job.title%>">
    </div>

    <div>
      Description: <input type="text" name="description" value="<%=@job.description%>">
    </div>


    <div class="checkbox">
      <input type="submit" class="btn btn-default" value="Edit Job Post">
    </div>
  </form>
<% end %>