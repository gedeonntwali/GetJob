<h1> All Jobs Available</h1>

<%= button_to "Add New Job Post", "/jobs/new", method: :get %>

<div class="container-fluid">
  <div class="row">
    <% @jobs.each do |job| %>
      <div class="col-sm-8 text-left"> 
        <h4><%= link_to job.title, "/jobs/#{job.id}" %></h4>
        <h4>Description: <%= job.description %></h4>
      </div>
    <% end %>
  </div>
</div>


